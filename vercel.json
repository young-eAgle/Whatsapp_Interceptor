{
  "version": 2,
  "functions": {
    "api/index.js": {
      "runtime": "nodejs20.x",
      "memory": 1024,
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.js"
    }
  ],
  "includeFiles": [
    "node_modules/@sparticuz/chromium/**"
  ],
  "env": {
    "IS_SERVERLESS": "true",
    "PUPPETEER_SKIP_CHROMIUM_DOWNLOAD": "true"
  }
}
