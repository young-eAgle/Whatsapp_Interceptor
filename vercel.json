{
  "version": 2,
  "functions": {
    "api/index.js": {
      "runtime": "nodejs20.x",
      "memory": 1024,
      "maxDuration": 10,
      "includeFiles":[
               "node_modules/chrome-aws-lambda/**/*"
      ]
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.js"
    }
  ],
    "env":{
      "IS_SERVERLESS":"true"
    }
  

}


